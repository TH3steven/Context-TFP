      <ImageView fx:id="cameraView" fitHeight="225.0" fitWidth="400.0" layoutX="31.0" layoutY="109.0" pickOnBounds="true" preserveRatio="true" />
      <ChoiceBox fx:id="selectCamera" layoutX="269.0" layoutY="35.0" prefHeight="25.0" prefWidth="44.0" />
      <TableView layoutX="457.0" layoutY="22.0" prefHeight="519.0" prefWidth="315.0">
        <columns>
          <TableColumn fx:id="presetColumn" prefWidth="66.0" text="Preset" />
          <TableColumn fx:id="descColumn" prefWidth="258.0" text="Description" />
        </columns>
      </TableView>
      <Label layoutX="32.0" layoutY="360.0" text="Preset ID:" />
      <ChoiceBox fx:id="presetID" layoutX="106.0" layoutY="356.0" prefHeight="25.0" prefWidth="44.0" />
      <Label layoutX="32.0" layoutY="405.0" text="Description:" />
      <TextField layoutX="106.0" layoutY="401.0" prefHeight="25.0" prefWidth="315.0" />
      <Button layoutX="344.0" layoutY="441.0" mnemonicParsing="false" text="Save Preset" />
